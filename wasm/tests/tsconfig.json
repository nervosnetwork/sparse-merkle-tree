{
    "compilerOptions": {
        "outDir": "ckb-contract/dist",
        "module": "ESNext",
        "target": "ES2020",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "resolveJsonModule": true
    }
}